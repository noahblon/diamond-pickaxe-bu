{
  "name": "starter-kit",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "babel --out-dir dist src",
    "lint": "eslint src",
    "precommit": "npm run validate",
    "test": "nyc blue-tape ./src/**/*.test.js | tap-diff",
    "validate": "npm-run-all --parallel lint test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/noahblon/starter-kit.git"
  },
  "author": "Noah Blon <noahblon@gmail.com> (http://noahblon.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/noahblon/starter-kit/issues"
  },
  "homepage": "https://github.com/noahblon/starter-kit#readme",
  "files": [
    "dist"
  ],
  "nyc": {
    "check-coverage": true,
    "branches": 100,
    "functions": 100,
    "lines": 100,
    "statements": 100,
    "reporter": [
      "text",
      "lcov"
    ],
    "include": [
      "src"
    ]
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "devDependencies": {
    "babel-cli": "6.18.0",
    "babel-preset-es2015": "6.18.0",
    "blue-tape": "1.0.0",
    "eslint": "3.12.2",
    "eslint-config-kentcdodds": "11.1.0",
    "husky": "0.12.0",
    "npm-run-all": "4.0.0",
    "nyc": "10.0.0",
    "rimraf": "2.5.4",
    "tap-diff": "0.1.1"
  }
}
