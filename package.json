{
  "name": "starter-kit",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "build": "npm-run-all --parallel build:*",
    "build:main": "babel --out-dir dist src",
    "build:umd": "webpack --output-filename index.umd.js",
    "build:umd.min": "webpack --output-filename index.umd.min.js -p",
    "lint": "eslint src",
    "prebuild": "rimraf dist",
    "precommit": "npm run validate",
    "report-coverage": "cat ./coverage/lcov.info | codecov",
    "test": "cross-env NODE_ENV=test nyc blue-tape ./src/**/*.test.js | tap-diff",
    "validate": "npm-run-all --parallel lint test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/noahblon/starter-kit.git"
  },
  "author": "Noah Blon <noahblon@gmail.com> (http://noahblon.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/noahblon/starter-kit/issues"
  },
  "homepage": "https://github.com/noahblon/starter-kit#readme",
  "files": [
    "dist"
  ],
  "devDependencies": {
    "babel-cli": "6.18.0",
    "babel-loader": "6.2.10",
    "babel-plugin-istanbul": "3.1.2",
    "babel-preset-es2015": "6.18.0",
    "babel-register": "6.18.0",
    "blue-tape": "1.0.0",
    "codecov": "1.0.1",
    "cross-env": "3.1.4",
    "eslint": "3.12.2",
    "eslint-config-kentcdodds": "11.1.0",
    "husky": "0.12.0",
    "npm-run-all": "4.0.0",
    "nyc": "10.0.0",
    "rimraf": "2.5.4",
    "tap-diff": "0.1.1",
    "webpack": "1.14.0"
  }
}
